}

<!--begin::Modal - Add task-->
<div class="modal fade" id="kt_modal_add_rank" tabindex="-1" aria-hidden="true">
	<!--begin::Modal dialog-->
	<div class="modal-dialog modal-dialog-centered mw-700px">
		<!--begin::Modal content-->
		<div class="modal-content">

			<!--begin::Modal header-->
			<div class="modal-header" id="kt_modal_add_rank_header">
				<!--begin::Modal title-->
				<h2 class="fw-bold" id="kt_add_rank_title">Add Rank</h2>
				<!--end::Modal title-->
				<!--begin::Close-->
				<div class="btn btn-icon btn-sm btn-active-icon-primary" data-kt-add-rank-modal-action="close">
					<i class="ki-duotone ki-cross fs-1">
						<span class="path1"></span>
						<span class="path2"></span>
					</i>
				</div>
				<!--end::Close-->
			</div>
			<!--end::Modal header-->
			<!--begin::Modal body-->
			<div class="modal-body px-5 my-7">

				<!--begin::Form-->
				<form id="kt_modal_add_rank_form" class="form" action="#">
					@* @Html.AntiForgeryToken() *@
					<!--begin::Scroll-->
					<div class="d-flex flex-column scroll-y px-5 px-lg-10" id="kt_modal_add_rank_scroll" data-kt-scroll="true" data-kt-scroll-activate="true" data-kt-scroll-max-height="auto" data-kt-scroll-dependencies="#kt_modal_add_rank_header" data-kt-scroll-wrappers="#kt_modal_add_rank_scroll" data-kt-scroll-offset="300px">

						<!--begin::Primary-->
						<input type="hidden" id="kt_rank_detno" name="detno" value="0">
						<!--end::Primary-->
						<!--begin::Rank Code-->
						<div class="fv-row mb-3">
							<!--begin::Label-->
							<label class="fs-6 fw-semibold form-label mt-3">
								<span class="required">Rank Code </span>
								<span class="ms-1" data-bs-toggle="tooltip" title="Enter the Rank Code.">
									<i class="ki-duotone ki-information fs-7">
										<span class="path1"></span>
										<span class="path2"></span>
										<span class="path3"></span>
									</i>
								</span>
							</label>
							<!--end::Label-->
							<!--begin::Input-->
							<input type="text" placeholder="Rank Code" class="form-control bg-transparent border border-primary border-hover border-active active" name="rank_code" id="kt_rank_code" autocomplete="off" value="" />
							<!--end::Input-->
						</div>
						<!--end::Rank Code-->
						<!--begin::Rank Name-->
						<div class="fv-row mb-3">
							<!--begin::Label-->
							<label class="fs-6 fw-semibold form-label mt-3">
								<span class="required">Rank Name</span>
								<span class="ms-1" data-bs-toggle="tooltip" title="Enter the Rank Name.">
									<i class="ki-duotone ki-information fs-7">
										<span class="path1"></span>
										<span class="path2"></span>
										<span class="path3"></span>
									</i>
								</span>
							</label>
							<!--end::Label-->
							<!--begin::Input-->
							<input type="text" placeholder="Rank Name" class="form-control bg-transparent border border-primary border-hover border-active active" name="rank_name" id="kt_rank_name" autocomplete="off" value="" />
							<!--end::Input-->
						</div>
						<!--end::Rank Name-->
					</div>
					<!--end::Scroll-->
					<!--begin::Separator-->
					<div class="separator mb-6"></div>
					<!--end::Separator-->
					<!--begin::Actions-->
					<div class="d-flex justify-content-end text-center pt-10">
						<button type="reset" class="btn btn-light me-3" data-kt-add-rank-modal-action="cancel">Discard</button>
						<button id="kt_add_rank_submit" type="submit" class="btn btn-primary" data-kt-add-rank-modal-action="submit">
							<span class="indicator-label">Submit</span>
							<span class="indicator-progress">
								Please wait...
								<span class="spinner-border spinner-border-sm align-middle ms-2"></span>
							</span>
						</button>
					</div>
					<!--end::Actions-->

				</form>
				<!--end::Form-->
			</div>
			<!--end::Modal body-->
		</div>
		<!--end::Modal content-->
	</div>
	<!--end::Modal dialog-->
</div>
<!--end::Modal - Add task-->